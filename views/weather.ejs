<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>5 day Weather Forecast for <%= cityUpperCased %></h1>

    <div id="mainContent">
        <% weather.list.forEach(({main:{temp_min, temp_max, humidity}, weather, wind:{speed}, dt_txt})=>{ %>
            <div class = "content">
            <p> Date: <%= dt_txt.slice(8,10) %>/<%= dt_txt.slice(5,7) %> Time: <%= dt_txt.slice(11,16) %></p>
                <%if (weather[0].description === 'overcast clouds') { %>
                    <img src="https://images.all-free-download.com/images/graphiclarge/tango_weather_overcast_115976.jpg" height="50" width="50">
                    <% } %>
    
                <%if (weather[0].description === 'light rain') { %>
                    <img src="https://previews.123rf.com/images/baldezh/baldezh1907/baldezh190700138/126428528-light-blue-cartoon-style-icon-of-summer-rain-with-cloud-isolated-on-white-background-illustration-of.jpg" height="50" width="50">
                    <% } %>
    
                <%if (weather[0].description === 'scattered clouds') { %>
                    <img src="https://lh3.googleusercontent.com/proxy/mC_nLWXUtH96WHvyp-treCKbSupJdW9XsTKkWD2AGDUHnz0d3p4c0_HBjdsTPcujaIaptxG26-9LAUuMsl6vOcpDPQsX9QE" height="50" width="50">
                    <% } %>
    
                <%if (weather[0].description === 'broken clouds') { %>
                    <img src="data:image/jpeg;base64," height="50" width="50">
                    <% } %>
    
                <%if (weather[0].description === 'clear sky') { %>
                    <img src="https://torange.biz/photofx/1/8/mirror-macro-blurring-top-bottom-sky-clear-1049.jpg" height="50" width="50">
                    <% } %>

                <%if (weather[0].description === 'moderate rain') { %>
                    <img src="https://s.pngkit.com/png/small/1-14654_cloud-rain-heavy-rain-cross-rain-comments-cloud.png" height="50" width="50">
                    <% } %>
    
                <%if (weather[0].description === 'few clouds') { %>
                    <img src="https://ak6.picdn.net/shutterstock/videos/1027902866/thumb/1.jpg?ip=x480" height="50" width="50">
                    <% } %>
                    
                <p>Theme: <%= weather[0].description %> </p>
                <p>Min Temp is <%= Math.round(temp_min - 273.15) %> °C</p>
                <p>Max Temp is <%= Math.round(temp_max - 273.15) %> °C</p>
                <p>Wind Speed: <%= speed %> mph </p>
                <p>Humidity: <%= humidity %>%</p>
                <!-- <img src=<%#img%> height="100" width="100" > -->
    
            </div>
    
            <%}) %> 
    </div>
    

    <br>
    <br>
   
  </body>
</html>
